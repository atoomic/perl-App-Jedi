#!/usr/bin/env perl

#Â ABSTRACT: Jedi App Manager
# PODNAME: jedi

use strict;
use warnings;
# VERSION
use feature 'say';

use FindBin qw/$Bin/;
use File::Spec;

my $default_root_dir = File::Spec->catfile($Bin, '..');
eval qq{use lib '$default_root_dir/lib/perl5'};

require App::Jedi;
App::Jedi->run($default_root_dir);
